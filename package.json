  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production DOMAIN_ENV=app.buildee.com npm run compile",
    "build-without-domain": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "qa": "npm run clean && cross-env NODE_ENV=production DOMAIN_ENV=qa.buildee.com npm run compile",
    "qa-graphql": "npm run clean && cross-env NODE_ENV=production DOMAIN_ENV=qa-graphql.buildee.com npm run compile",
    "beta": "npm run clean && cross-env NODE_ENV=production DOMAIN_ENV=beta.buildee.com npm run compile",
    "start": "cross-env NODE_ENV=development DOMAIN_ENV=localhost webpack-dev-server --open --mode development --config build/webpack.config.js",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --open --mode development --config build/webpack.config.js",
    "test": "echo 'success'",
    "test:watch": "npm test -- --watch",
    "test:integration": "cross-env NODE_ENV=test cypress run",
    "format": "prettier --write",
    "format:all": "yarn format \"src/**/*.{js,jsx}\"",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "cypress:open": "cross-env NODE_ENV=test cypress open"
  },
